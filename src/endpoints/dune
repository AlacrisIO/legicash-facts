;; dune file for Legicash endpoints demo
(library
 (name endpoints)
 (modules accounts actions endpoints)
 (preprocess (pps ppx_deriving.std ppx_deriving_yojson))
 ;; -9 : all fields mentioned in a pattern
 ;; -27 : unused variable
 ;; -32 : unused value
 ;; -34 : unused type
 ;; -39 : unused rec (@@deriving yojson yields these)
 (flags (:standard -w -9-34-39 -warn-error -27-32-33 -I ../../../_build/default/.legicash_lib.objs))
 (libraries base64 cohttp-lwt-unix eliom.server integers lwt ppx_deriving_yojson.runtime secp256k1 yojson))
